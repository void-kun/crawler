# All time from config is in seconds
concurrency: 1
delay: 1
user_agent:
  - "Mozilla/5.0 (X11; Linux x86_64; rv:128.0) Gecko/20100101 Firefox/128.0"
max_depth: 0
browser_path: ""
browser_timeout: 120
proxy_url: ""
output_dir: "./output"
session_file: "./session_data.json"
stv:
  username:
  password:
  origin: https://sangtacviet.app

# RabbitMQ configuration
rabbitmq:
  url: "amqp://guest:guest@localhost:5672/"
  queue_name: "crawler_tasks"
  exchange_name: "crawler_exchange"
  exchange_type: "topic"
  routing_keys:
    - "crawl.sangtacviet.book"
    - "crawl.sangtacviet.chapter"
    - "crawl.sangtacviet.session"
  priority_topic: "crawl.sangtacviet.session"
  prefetch_count: 1
  reconnect_interval: 5 # seconds

# Control API configuration
control_api:
  base_url: "http://localhost:8080"
  timeout: 30 # seconds
  api_key: ""
  report_results: true
  results_endpoint: "/api/task-results"
